generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SecretKey {
  pcId String @id @default(uuid()) @db.Uuid
  PcName String
  WmicUUID String
}

model User {
  userId String @id @default(uuid()) @db.Uuid
  login String @unique
  password String
  name String
  phoneNumber String @unique
  email String @unique
  address String
}